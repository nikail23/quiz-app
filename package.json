{
  "build": {
    "appId": "com.example.app",
    "asar": true,
    "files": [
      "dist/quiz-app/browser/**/*",
      "main.js"
    ],
    "win": {
      "target": [
        "portable"
      ]
    }
  },
  "name": "quiz-app",
  "author": "Ilya Yermalovich",
  "description": "App for quiz",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng build --base-href ./ && electron .",
    "build": "ng build --production --base-href ./",
    "test": "ng test",
    "electron": "electron .",
    "package-win-x64": "ng build --configuration production --base-href ./ && electron-builder --win --x64",
    "package-win-x32": "ng build --configuration production --base-href ./ && electron-builder --win --ia32"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.6",
    "@angular/cli": "^19.2.6",
    "@angular/compiler-cli": "^19.2.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.7.2",
    "electron": "23.3.13",
    "electron-builder": "^26.0.12",
    "electron-packager": "^17.1.2"
  }
}
